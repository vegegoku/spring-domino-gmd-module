<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:m='urn:import:gwt.material.design.client.ui'
>

    <ui:style gss="true">
        .toleft{
            text-align : left !important;
            border: 0px !important;
        }
    </ui:style>

    <m:MaterialPanel>
        <m:MaterialContainer ui:field="listContainer">
            <m:MaterialCollection ui:field="itemsCollection" addStyleNames="{style.toleft}">

            </m:MaterialCollection>
        </m:MaterialContainer>
    </m:MaterialPanel>

</ui:UiBinder>